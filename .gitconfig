[user]
	name = Dave Kleinschmidt
	email = dave.f.kleinschmidt@gmail.com
[core]
	excludesfile = /Users/dkleinschmidt/.gitignore_global
	editor = emacs
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Volumes/SourceTree/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = matching

[url "git@github.com:"]
	insteadof = "https://github.com/"
[github]
	user = kleinschmidt
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[alias]
	graph = log --oneline --decorate --graph
